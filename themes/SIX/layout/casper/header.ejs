            <div class="navbar-inner">
                <div class="container">
                    <!-- Nav logo -->
                    <div class="logo">
                        <a href="/" title="Logo" class="animsition-link">
                        <% if (theme.logo) { %> <img src="<%- theme.logo %>" alt="Logo" width="35px;"/> <% } %>
                        </a>
                    </div>
                    <!-- // Nav logo -->
                    <!-- Info-bar -->
                    <nav>
                        <ul class="nav">
                            <li><a href="/" class="animsition-link"><%- config.title %></a></li>
                            <li class="nolink"><span>Always </span>Creative.</li>
                            <% if(theme.github){ %>
                            <li><a href="<%- theme.github %>" title="Github" target="_blank"><i class="icon-github"></i></a></li>
                            <% } %>
                            <% if(theme.twitter){ %>
                            <li><a href="<%- theme.twitter %>" title="Twitter" target="_blank"><i class="icon-tuite"></i></a></li>
                            <% } %>
                            <% if(theme.weibo){ %>
                            <li><a href="<%- theme.weibo %>" title="Sina-Weibo" target="_blank"><i class="icon-zliconweibo01"></i></a></li>
                            <% } %>
                            <% if(theme.bokeyuan){ %>
                            <li><a href="<%- theme.bokeyuan %>" title="cnblogs" target="_blank"><i class="icon-wuxianxinhao"></i></a></li>
                            <% } %>
                            <% if(theme.csdn){ %>
                            <li><a href="<%- theme.csdn %>" title="csdn" target="_blank"><i class="icon-c"></i></a></li>
                            <% } %>
                            <li class="nolink"><span>Welcome!</span></li>
                        </ul>
                    </nav>
                    <!--// Info-bar -->
                </div>
                <!-- // .container -->
                <div class="learnmore sb-toggle-right">More</div>
